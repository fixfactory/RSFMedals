{
    "manifest_version":3,
    "version":"1.3",
    "name":"RSF Medals",
    "icons": {
      "48": "icons/icon48.png",
      "96": "icons/icon96.png"
    },
    "content_scripts":[
      {
       "matches":["*://*.rallysimfans.hu/rbr/usersstats.php*"],
       "js":["main.js"]
      }
    ],
    "host_permissions": [
      "*://*.rallysimfans.hu/rbr/usersstats.php*"
    ],
    "web_accessible_resources": [
      {
        "matches": ["*://*.rallysimfans.hu/*"],
        "resources": ["images/wr.png", "images/gold.png", "images/silver.png", "images/bronze.png"]
      }
    ] 
  }
  